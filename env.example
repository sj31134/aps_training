# Autodesk APS (Forge) 인증 정보
# https://aps.autodesk.com/ 에서 Client ID와 Secret을 발급받으세요

# APS Client ID
APS_CLIENT_ID=your_client_id_here

# APS Client Secret
APS_CLIENT_SECRET=your_client_secret_here

# APS Callback URL (선택사항)
APS_CALLBACK_URL=http://localhost:3000/api/auth/callback

# APS Base URL (기본값: https://developer.api.autodesk.com)
APS_BASE_URL=https://developer.api.autodesk.com

# 환경 설정
NODE_ENV=development

# 포트 설정 (선택사항)
PORT=3000


